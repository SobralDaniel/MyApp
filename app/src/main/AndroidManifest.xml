<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nbacademy.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.SignUpActivity"/>
        <activity android:name=".TripDetailsActivity" />
        <activity android:name=".TripCategoriesActivity" />
        <activity android:name=".DestinyActivity" />
        <activity android:name=".NewDestinyActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".MainActivity_2"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".Activitys.MainActivity_Activitys"
            android:parentActivityName=".MainActivity_2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity_2" />
        </activity>
        <activity
            android:name=".Activitys.SuggestionsActivity"
            android:label="@string/title_activity_meu_percurso"
            android:parentActivityName=".Activitys.MainActivity_Activitys"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nbacademy.myapp.Activitys.MainActivity_Activitys" />
        </activity>
        <activity
            android:name=".Activitys.MeuPercurso"
            android:label="@string/title_activity_meu_percurso"
            android:parentActivityName=".Activitys.MainActivity_Activitys"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nbacademy.myapp.Activitys.MainActivity_Activitys" />
        </activity>
        <activity
            android:label="Nova Actividade"
            android:name=".Activitys.NewActivity" />
        <activity
            android:name=".Activitys.AlimentacaoActivity" />
    </application>

    <meta-data
        android:name="com.facebook.sdk.ApplicationId"
        android:value="@string/facebook_app_id"/>


</manifest>